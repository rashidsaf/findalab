{
  "name": "medology/findalab",
  "description": "Lab finder component for the Medology platform.",
  "keywords": ["jquery", "laravel", "lumen"],
  "license": "private",
  "type": "project",
    "require": {
      "php": ">=5.5.9",
      "laravel/lumen-framework": "5.2.*",
      "vlucas/phpdotenv": "~2.2"
  },
  "require-dev": {
    "behat/behat": "dev-v3.4.3-with-patches as 3.4.3",
    "behat/mink-extension": "^2.0",
    "behat/mink-selenium2-driver": "dev-1.3.1-with-patches as 1.3.1",
    "fzaninotto/faker": "~1.4",
    "jarnaiz/behat-junit-formatter": "^1.3",
    "medology/flexible-mink": "1.0.x-dev as dev-master",
    "phpunit/phpunit": "~4.0",
    "vlucas/phpdotenv": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "dev/app/",
      "features\\": "features/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "vendor-dir": "dev/vendor"
  },
  "autoload-dev": {
    "classmap": [
      "dev/database/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Chekote/MinkSelenium2Driver"
    }
  ]
}
